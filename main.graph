pow :: (value: int, power: int) -> int {
    if power == 0 {
        <- 1;
    }
    if power == 1 {
        <- value;
    }

    counter := 0;
    total := value;
    while counter < power - 1 {
        total = total * value;
        counter = counter + 1;
    }
    <- total;
}

main :: (){
    counter := 0;

    while counter < 50 {
        print(pow(2, counter));
        counter = counter + 1;
    }
}

